//					sum = (int)Math.pow(digits[i], digits[0]);
//				}
//				else
//				{
//					sum=(int)Math.pow(digits[i],digits[i+1]);
//					
//				}
//					
//				
//			}
			System.out.println(sum);
		
		return sum;
	}

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		System.out.println("Enter Integer");
		int a=scan.nextInt();
		sum(a);
	}
}
